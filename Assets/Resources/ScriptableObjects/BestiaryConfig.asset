  - MonsterName: "\u0412\u0438\u0439"
    MonsterMaterial: {fileID: 2100000, guid: 399551f8969eeaf41b8a18b9bd40904b, type: 2}
    MonsterDesc: "<b>\u0421\u043B\u0430\u0431\u043E\u0441\u0442\u0438:</b>\n\u2014
      <b>\u041D\u0435 \u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C \u0432 \u043B\u0438\u0446\u043E:</b>
      \u0415\u0441\u043B\u0438 \u0412\u0438\u0439 \u043E\u0442\u043A\u0440\u043E\u0435\u0442
      \u0433\u043B\u0430\u0437\u0430 (\u0434\u0430\u0436\u0435 \u0447\u0430\u0441\u0442\u0438\u0447\u043D\u043E),
      \u0437\u0440\u0438\u0442\u0435\u043B\u044C \u0442\u0435\u0440\u044F\u0435\u0442
      \u0432\u043E\u043B\u044E \u0438 \u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0441\u044F
      \u0435\u0433\u043E \u0440\u0430\u0431\u043E\u043C.  \n\u2014 <b>\u041D\u0435
      \u043E\u0442\u0432\u0435\u0447\u0430\u0442\u044C \u043D\u0430 \u0433\u043E\u043B\u043E\u0441:</b>
      \u0417\u043E\u0432\u0451\u0442 \u0433\u043E\u043B\u043E\u0441\u0430\u043C\u0438
      \u0443\u043C\u0435\u0440\u0448\u0438\u0445, \u0437\u0430\u043C\u0430\u043D\u0438\u0432\u0430\u044F
      \u0436\u0435\u0440\u0442\u0432\u0443 \u0431\u043B\u0438\u0436\u0435. \u041E\u0442\u0432\u0435\u0442
      \u2014 \u043F\u0443\u0442\u044C \u0432 \u0435\u0433\u043E \u0446\u0430\u0440\u0441\u0442\u0432\u043E. 
      \n\u2014 <b>\u0421\u0438\u043B\u0430 \u0432\u043E\u043B\u0438: </b>\u0415\u0434\u0438\u043D\u0441\u0442\u0432\u0435\u043D\u043D\u0430\u044F
      \u0437\u0430\u0449\u0438\u0442\u0430 \u2014 \u043D\u0435 \u0434\u0430\u0442\u044C
      \u0441\u0442\u0440\u0430\u0445\u0443 \u0432\u0437\u044F\u0442\u044C \u0432\u0435\u0440\u0445.
      \u0421\u0442\u0440\u0430\u0445 \u0434\u0435\u043B\u0430\u0435\u0442 \u0432\u0430\u0441
      \u0443\u044F\u0437\u0432\u0438\u043C\u044B\u043C.\n\n<b>\u041E\u0441\u043E\u0431\u0435\u043D\u043D\u043E\u0441\u0442\u0438:</b>\n\u2014
      \u0422\u0435\u043B\u043E \u2014 \u0441\u043F\u043B\u0435\u0442\u0435\u043D\u0438\u0435
      \u0434\u0435\u0440\u0435\u0432\u0430 \u0438 \u043F\u043B\u043E\u0442\u0438:
      \u0432\u0435\u0442\u0432\u0438 \u043F\u0440\u043E\u0431\u0438\u0432\u0430\u044E\u0442\u0441\u044F
      \u0441\u043A\u0432\u043E\u0437\u044C \u0433\u0440\u0443\u0434\u044C, \u043A\u043E\u0440\u043D\u0438
      \u0442\u043E\u0440\u0447\u0430\u0442 \u0438\u0437 \u0441\u0443\u0441\u0442\u0430\u0432\u043E\u0432,
      \u043A\u043E\u0436\u0430 \u043F\u043E\u043A\u0440\u044B\u0442\u0430 \u043B\u0438\u0448\u0430\u0439\u043D\u0438\u043A\u043E\u043C. 
      \n\u2014 \u0412\u0435\u043A\u0438 \u2014 \u0442\u044F\u0436\u0451\u043B\u044B\u0435,
      \u0441\u043C\u043E\u0440\u0449\u0435\u043D\u043D\u044B\u0435, \u043E\u043F\u0443\u0449\u0435\u043D\u044B
      \u0434\u043E \u0437\u0435\u043C\u043B\u0438, \u043D\u043E \u043F\u043E\u0434
      \u043D\u0438\u043C\u0438 \u043F\u0440\u043E\u0441\u0442\u0443\u043F\u0430\u044E\u0442
      \u0441\u0438\u043B\u0443\u044D\u0442\u044B \u0433\u043B\u0430\u0437, \u043C\u043D\u043E\u0436\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0445,
      \u0431\u0443\u0434\u0442\u043E \u0441\u043E\u0431\u0440\u0430\u043D\u043D\u044B\u0445
      \u0438\u0437 \u0440\u0430\u0437\u0431\u0438\u0442\u044B\u0445 \u0437\u0435\u0440\u043A\u0430\u043B. 
      \n\u2014 \u0412\u0438\u0434\u0438\u0442 \u043C\u044B\u0441\u043B\u0438, \u0441\u043B\u044B\u0448\u0438\u0442
      \u0441\u0442\u0440\u0430\u0445\u0438. \u0415\u0433\u043E \u043F\u0440\u0438\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u0435
      \u0432\u044B\u0437\u044B\u0432\u0430\u0435\u0442 \u0433\u0430\u043B\u043B\u044E\u0446\u0438\u043D\u0430\u0446\u0438\u0438
      \u2014 \u0432\u0438\u0434\u0435\u043D\u0438\u044F \u0438\u0437 \u043F\u0440\u043E\u0448\u043B\u043E\u0433\u043E
      \u0436\u0435\u0440\u0442\u0432\u044B.\n\n<b>\u041E\u043F\u0430\u0441\u043D\u043E\u0441\u0442\u0438:</b>\n\u2014
      <b>\u0416\u0435\u0440\u0442\u0432\u043E\u043F\u0440\u0438\u043D\u043E\u0448\u0435\u043D\u0438\u0435:</b>
      \u0422\u0440\u0435\u0431\u0443\u0435\u0442 \u043A\u0440\u043E\u0432\u044C \u043E\u0441\u0435\u043D\u044C\u044E.
      \u0415\u0441\u043B\u0438 \u043D\u0435 \u043F\u0440\u0438\u043D\u0435\u0441\u0451\u043D
      \u0434\u0430\u0440 \u2014 \u0437\u0430\u0431\u0438\u0440\u0430\u0435\u0442
      \u0432\u0441\u0435\u0445.  \n\u2014 <b>\u0417\u043E\u0432 \u0438\u043C\u0435\u043D\u0438:</b>
      \u0415\u0441\u043B\u0438 \u0412\u0438\u0439 \u043D\u0430\u0437\u043E\u0432\u0451\u0442
      \u0432\u0430\u0448\u0435 \u0438\u043C\u044F, \u0432\u044B \u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435\u0441\u044C
      \u0435\u0433\u043E. \u041E\u043D \u0437\u043D\u0430\u0435\u0442 \u0438\u043C\u0435\u043D\u0430
      \u0442\u0435\u0445, \u043A\u0442\u043E \u043F\u0440\u043E\u0448\u0451\u043B
      \u0447\u0435\u0440\u0435\u0437 \u043B\u0435\u0441.  \n\u2014 <b>\u041D\u0435\u0443\u044F\u0437\u0432\u0438\u043C\u043E\u0441\u0442\u044C:</b>
      \u041E\u0433\u043E\u043D\u044C \u043D\u0435 \u0431\u0435\u0440\u0451\u0442,
      \u0436\u0435\u043B\u0435\u0437\u043E \u043D\u0435 \u0440\u0435\u0436\u0435\u0442.
      \u0422\u043E\u043B\u044C\u043A\u043E \u0432\u043E\u043B\u044F \u043C\u043E\u0436\u0435\u0442
      \u043E\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u044C \u0435\u0433\u043E.\n\n<b>\u041E\u043F\u0438\u0441\u0430\u043D\u0438\u0435:</b>\n<i>\u0412\u0438\u0439
      \u2014 \u0438\u0441\u043F\u043E\u043B\u0438\u043D, \u0432\u044B\u0448\u0435
      \u0447\u0435\u043B\u043E\u0432\u0435\u043A\u0430 \u043D\u0430 \u0434\u0432\u0435
      \u0433\u043E\u043B\u043E\u0432\u044B. \u0415\u0433\u043E \u0442\u0435\u043B\u043E
      \u2014 \u0434\u0435\u0440\u0435\u0432\u043E, \u0432\u0440\u043E\u0441\u0448\u0435\u0435
      \u0432 \u043F\u043B\u043E\u0442\u044C: \u0432\u0435\u0442\u0432\u0438 \u0442\u043E\u0440\u0447\u0430\u0442
      \u0438\u0437 \u0433\u0440\u0443\u0434\u0438, \u043A\u043E\u0440\u043D\u0438
      \u0432\u044C\u044E\u0442\u0441\u044F \u0432\u043E\u043A\u0440\u0443\u0433 \u0440\u0443\u043A,
      \u0430 \u043A\u043E\u0436\u0430 \u043F\u043E\u043A\u0440\u044B\u0442\u0430
      \u043B\u0438\u0448\u0430\u0439\u043D\u0438\u043A\u043E\u043C, \u043A\u0430\u043A
      \u043A\u043E\u0440\u0430 \u0441\u0442\u0430\u0440\u043E\u0433\u043E \u0434\u0443\u0431\u0430.
      \u0412\u0435\u043A\u0438, \u0442\u044F\u0436\u0451\u043B\u044B\u0435 \u0438
      \u0441\u043C\u043E\u0440\u0449\u0435\u043D\u043D\u044B\u0435, \u0432\u043E\u043B\u043E\u0447\u0430\u0442\u0441\u044F
      \u043F\u043E \u0437\u0435\u043C\u043B\u0435, \u043D\u043E \u043F\u043E\u0434
      \u043D\u0438\u043C\u0438 \u0441\u043A\u0440\u044B\u0432\u0430\u044E\u0442\u0441\u044F
      \u0433\u043B\u0430\u0437\u0430, \u043C\u043D\u043E\u0436\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0435,
      \u0431\u0443\u0434\u0442\u043E \u0441\u043E\u0431\u0440\u0430\u043D\u043D\u044B\u0435
      \u0438\u0437 \u043E\u0441\u043A\u043E\u043B\u043A\u043E\u0432 \u0437\u0435\u0440\u043A\u0430\u043B.
      \u0415\u0433\u043E \u0448\u0430\u0433\u0438 \u0441\u043E\u0442\u0440\u044F\u0441\u0430\u044E\u0442
      \u0437\u0435\u043C\u043B\u044E, \u0430 \u0433\u043E\u043B\u043E\u0441 \u2014
      \u043D\u0438\u0437\u043A\u0438\u0439, \u0434\u0440\u043E\u0436\u0430\u0449\u0438\u0439,
      \u0431\u0443\u0434\u0442\u043E \u0441\u043A\u0430\u043B\u0430 \u0440\u0430\u0441\u043A\u0430\u043B\u044B\u0432\u0430\u0435\u0442\u0441\u044F
      \u0432\u043D\u0443\u0442\u0440\u0438.</i>\n\n<b>\u0418\u0437 \u0434\u043D\u0435\u0432\u043D\u0438\u043A\u0430
      \u0441\u0442\u0430\u0440\u043E\u0441\u0442\u044B:</b>\n<i>\xAB\u041D\u0435
      \u0441\u043C\u043E\u0442\u0440\u0438 \u0435\u043C\u0443 \u0432 \u043B\u0438\u0446\u043E.
      \u041D\u0435 \u043E\u0442\u0432\u0435\u0447\u0430\u0439 \u043D\u0430 \u0437\u043E\u0432.
      \u041E\u043D \u0441\u043B\u0435\u043F, \u043D\u043E \u0432\u0438\u0434\u0438\u0442
      \u0432\u0441\u0451. \u041E\u043D \u043C\u0451\u0440\u0442\u0432, \u043D\u043E
      \u0436\u0438\u0432\u0451\u0442 \u0441\u0440\u0435\u0434\u0438 \u043D\u0430\u0441.
      \u041A\u0430\u0436\u0434\u0443\u044E \u043E\u0441\u0435\u043D\u044C \u2014
      \u043E\u0434\u043D\u0430 \u0434\u0443\u0448\u0430. \u0418\u043D\u0430\u0447\u0435
      \u0437\u0430\u0431\u0435\u0440\u0451\u0442 \u0432\u0441\u0435\u0445. \u041F\u0440\u043E\u0441\u0442\u0438,
      \u0413\u043E\u0441\u043F\u043E\u0434\u0438, \u043F\u0440\u043E\u0441\u0442\u0438\u2026\xBB</i>"
